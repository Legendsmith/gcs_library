//JSON export template by legendsmith
//Legendsmith#1102 on discord
//legendsmith.au@gmail.com
//Anything that may output nothing is wrapped in a string to maintain valid data structure, this includes skill levels because skill containers in GCS output a skill with no skill level.
{
	"name":"@NAME",
	"points":@TOTAL_POINTS,
	"ST":[@ST,@ST_POINTS],
	"DX":[@DX,@DX_POINTS],
	"IQ":[@IQ,@IQ_POINTS],
	"HT":[@HT,@HT_POINTS],
	"thr":"@THRUST",
	"sw":"@SWING",
	"bl":"@BASIC_LIFT",
	"HP":[@BASIC_HP,@HP_POINTS],
	"FP":[@BASIC_FP,@FP_POINTS],
	"will":[@WILL,@WILL_POINTS],
	"per":[@PERCEPTION,@PERCEPTION_POINTS],
	"speed":[@BASIC_SPEED,@BASIC_SPEED_POINTS],
	"move":[@BASIC_MOVE,@BASIC_MOVE_POINTS],
	"advantages":[
		@ADVANTAGES_ONLY_LOOP_START
		["@DESCRIPTION_PRIMARY",@POINTS],
		@ADVANTAGES_ONLY_LOOP_END
		null
	],
	"perks":[
		@PERKS_LOOP_START
		["@DESCRIPTION_PRIMARY",@POINTS],
		@PERKS_LOOP_END
		null
	],
	"disadvantages":[
		@DISADVANTAGES_LOOP_START
		["@DESCRIPTION_PRIMARY",@POINTS],
		@DISADVANTAGES_LOOP_END
		null
	],
	"quirks":[
		@QUIRKS_LOOP_START
		["@DESCRIPTION_PRIMARY",@POINTS],
		@QUIRKS_LOOP_END
		null
	],
	"skills":[
		@SKILLS_LOOP_START
		["@DESCRIPTION_PRIMARY",@SL,"@RSL","@POINTS"],
		@SKILLS_LOOP_END
		null
	],
	"equipment":[
		@EQUIPMENT_LOOP_START
		["@DESCRIPTION_PRIMARY",@QTY,"@COST","@WEIGHT"],
		@EQUIPMENT_LOOP_END
		null
	],
	"melee":[
	@MELEE_LOOP_START
	{"name":"@DESCRIPTION_PRIMARY", "usage":"@USAGE","level":@LEVEL,"parry":"@PARRY","block":"@BLOCK","damage":"@DAMAGE","reach":"@REACH","st":"@STRENGTH"},
	@MELEE_LOOP_END
	null
	],
	"range":[
	@RANGED_LOOP_START
	{"name":"@DESCRIPTION_PRIMARY", "usage":"@USAGE","level":@LEVEL,"acc":@ACCURACY,"range":"@RANGE","damage":"@DAMAGE","rof":"@ROF","st":"@STRENGTH","bulk":"@BULK","recoil":"@RECOIL"},
	@RANGED_LOOP_END
	null
	]
}
